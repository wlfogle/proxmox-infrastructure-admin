{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "Proxmox Infrastructure Admin",
  "version": "0.1.0",
  "identifier": "com.proxmox.infrastructure.admin",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1421",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 800,
        "resizable": true,
        "title": "Proxmox Infrastructure Admin",
        "width": 1200,
        "minWidth": 800,
        "minHeight": 600
      }
    ]
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "targets": "all"
  },
  "plugins": {}
}